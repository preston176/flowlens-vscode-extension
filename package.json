{
  "name": "flowlens",
  "displayName": "FlowLens",
  "description": "Instantly resume your coding flow â€” capture and restore your VS Code sessions with open files, terminals, git branch, and notes",
  "version": "0.0.1",
  "publisher": "preston176",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "productivity",
    "context",
    "session",
    "flow",
    "developer-tools",
    "workspace",
    "context-switching",
    "restore",
    "capture"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "galleryBanner": {
    "color": "#007ACC",
    "theme": "dark"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "FlowLens.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "FlowLens.captureSession",
        "title": "FlowLens: Capture Session"
      },
      {
        "command": "FlowLens.showSessions",
        "title": "FlowLens: Show Sessions"
      }
      ,
      {
        "command": "FlowLens.openSessionsPanel",
        "title": "FlowLens: Open Sessions Panel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
  "build:webview": "node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "esbuild": "^0.25.10",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/preston176/flowlens-vscode-extension.git"
  },
  "bugs": {
    "url": "https://github.com/preston176/flowlens-vscode-extension/issues"
  },
  "homepage": "https://flowlens-vscode.vercel.app",
  "qna": "https://github.com/preston176/flowlens-vscode-extension/discussions"
}
